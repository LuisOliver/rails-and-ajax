<%= form_for(@project, :remote => true, :url => {:action => url}) do |f| %>
  <div id= "project_errors" style="display:none"></div>
  <table>
    <tbody>
    <tr>
      <td>
        <%= f.label :name %>
      </td>
      <td>
        <%= f.text_field :name%>
      </td>
    </tr>
    <tr>
      <td>
        <%= f.label :description %>
      </td>
      <td>
        <%= f.text_area :description%>
      </td>
    </tr>
    <tr>
      <td colspan="2">
        <%= f.submit %>
      </td>
    </tr>
    </tbody>
  </table>
<% end %>
